{
  "name": "@iziwork/consent-manager",
  "version": "4.3.2",
  "description": "Drop-in consent management plugin for analytics.js",
  "keywords": [
    "gdpr",
    "tracking",
    "analytics",
    "analytics.js"
  ],
  "repository": "git://github.com/Iziwork/consent-manager.git",
  "license": "MIT",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "commonjs",
    "esm",
    "standalone"
  ],
  "scripts": {
    "test": "jest src/__tests__",
    "prepublishOnly": "npm run clean && npm run build",
    "postpublish": "npm run build-storybook && npm run deploy-storybook -- --existing-output-dir=storybook-static",
    "dev": "concurrently 'npm run build-standalone --watch' 'npm run storybook -s ./ -p 9009'",
    "build-commonjs": "tsc --outDir commonjs --module CommonJS --inlineSourceMap",
    "build-esm": "tsc --module es2015 --outDir esm --inlineSourceMap",
    "build-standalone": "webpack",
    "build": "concurrently --names 'commonjs,esm,standalone' 'npm run build-commonjs' 'npm run build-esm' 'npm run build-standalone'",
    "clean": "rm -rf commonjs esm standalone storybook-static",
    "deploy-storybook": "storybook-to-ghpages",
    "standalone-hash": "shasum -b -a 256 standalone/consent-manager.js | xxd -r -p | base64",
    "build-storybook": "npm run build-standalone && build-storybook && cp -r ./standalone ./storybook-static/standalone",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "storybook": "start-storybook",
    "size-limit": "size-limit"
  },
  "dependencies": {
    "@segment/in-regions": "1.2.2",
    "@segment/top-domain": "3.0.0",
    "emotion": "9.2.12",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.1",
    "lodash": "4.17.15",
    "nanoid": "2.1.11",
    "prop-types": "15.7.2",
    "react-emotion": "9.2.12"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@segment/in-eu": "0.3.0",
    "@size-limit/preset-small-lib": "4.3.1",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-info": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addon-options": "5.3.14",
    "@storybook/addon-storysource": "5.3.14",
    "@storybook/addons": "5.3.14",
    "@storybook/react": "5.3.14",
    "@storybook/storybook-deployer": "2.8.3",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "25.1.4",
    "@types/js-cookie": "2.2.5",
    "@types/lodash": "4.14.149",
    "@types/nanoid": "2.1.0",
    "@types/nock": "11.1.0",
    "@types/node": "12.12.30",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/segment-analytics": "0.0.32",
    "@types/sinon": "7.5.2",
    "@types/storybook__react": "5.2.1",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "babel-loader": "8.0.6",
    "concurrently": "5.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "evergreen-ui": "4.23.2",
    "file-loader": "5.1.0",
    "husky": "4.2.3",
    "jest": "24.9.0",
    "lint-staged": "10.0.8",
    "lodash-es": "4.17.15",
    "nock": "12.0.2",
    "preact": "10.3.4",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "prismjs": "1.19.0",
    "raw-loader": "4.0.0",
    "react": "16.13.0",
    "react-docgen-typescript-loader": "3.7.1",
    "react-dom": "16.13.0",
    "react-simple-code-editor": "0.11.0",
    "react-syntax-highlighter": "12.2.1",
    "sinon": "9.0.1",
    "size-limit": "4.3.1",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  },
  "peerDependencies": {
    "react": "16.13.0",
    "react-dom": "16.13.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./test/setupTests.ts"
    ]
  },
  "size-limit": [
    {
      "path": "esm/index.js",
      "limit": "50 KB"
    },
    {
      "path": "standalone/consent-manager.js",
      "limit": "45 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "storybook-deployer": {
    "gitUsername": "GitHub Pages Bot",
    "gitEmail": "friends@iziwork.com",
    "commitMessage": "Deploy Storybook to GitHub Pages [skip ci]"
  }
}
