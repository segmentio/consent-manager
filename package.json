{
  "name": "@segment/consent-manager",
  "version": "1.3.1",
  "description": "Drop-in consent management plugin for analytics.js",
  "keywords": [
    "gdpr",
    "tracking",
    "analytics",
    "analytics.js"
  ],
  "repository": "segmentio/consent-manager",
  "license": "MIT",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "commonjs",
    "esm",
    "standalone"
  ],
  "scripts": {
    "test": "jest",
    "prepublishOnly": "yarn run clean && yarn run build",
    "postpublish": "yarn run deploy-storybook",
    "dev": "start-storybook -p 9009",
    "build-commonjs": "tsc --outDir commonjs --inlineSourceMap",
    "build-esm": "tsc --module es2015 --outDir esm --inlineSourceMap",
    "build-standalone": "BABEL_ENV=esm webpack",
    "build": "concurrently --names 'commonjs,esm,standalone' 'yarn run build-commonjs' 'yarn run build-esm' 'yarn run build-standalone'",
    "clean": "rm -rf commonjs esm standalone",
    "deploy-storybook": "storybook-to-ghpages",
    "standalone-hash": "shasum -b -a 256 standalone/consent-manager.js | xxd -r -p | base64",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@segment/top-domain": "^3.0.0",
    "babel-runtime": "^6.26.0",
    "emotion": "^9.1.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.5",
    "nanoid": "^1.0.2",
    "prop-types": "^15.6.1",
    "react-emotion": "^9.1.2"
  },
  "devDependencies": {
    "@segment/in-eu": "^0.3.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addon-options": "^3.4.1",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.18",
    "@types/js-cookie": "^2.2.2",
    "@types/lodash": "^4.14.141",
    "@types/nanoid": "^2.1.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/sinon": "^7.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-emotion": "^9.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-react": "^7.7.0",
    "evergreen-ui": "^3.0.0",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^7.0.4",
    "lodash-es": "^4.17.10",
    "nock": "^9.2.5",
    "preact": "^10.0.0",
    "preact-compat": "^3.19.0",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "sinon": "^5.0.7",
    "size-limit": "^0.17.0",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./test/setupTests.ts"
    ]
  },
  "size-limit": [
    {
      "path": "esm/index.js",
      "limit": "48 KB"
    },
    {
      "path": "standalone/consent-manager.js",
      "limit": "44 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "storybook-deployer": {
    "gitUsername": "GitHub Pages Bot",
    "gitEmail": "friends@segment.com",
    "commitMessage": "Deploy Storybook to GitHub Pages [skip ci]"
  }
}
